{
  "name": "backend-server",
  "version": "1.0.0",
  "description": "node server backend",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "server-prod": "node app.js",
    "server-dev": "npx nodemon app.js",
    "client": "npm start --prefix ../react-client",
